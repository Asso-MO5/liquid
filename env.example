# Variables d'environnement pour l'association MO5

# Base de données
DATABASE_URL=mysql://root:password@localhost:3306/mo5_association

# Authentification Discord
DISCORD_ID=your_discord_client_id
DISCORD_SECRET=your_discord_client_secret
DISCORD_TOKEN=your_discord_bot_token
DISCORD_GUILD_ID=your_discord_guild_id

# Auth.js
AUTH_SECRET=your_auth_secret_key_here
AUTH_TRUST_HOST=true
AUTH_URL=http://localhost:3000

# Chiffrement des plaintes
ENCRYPTION_KEY=your_very_secure_encryption_key_here_at_least_32_characters

# Environnement
NODE_ENV=development

# Variables côté client (VITE_*)
# API Backend (Ocelot)
VITE_API_URL=http://localhost:3000

# Blog
VITE_BLOG_URL=http://localhost:3000

# Cave (si applicable)
VITE_CAVE_URL=http://localhost:3000

# Stripe (clé publique)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Serveur Colyseus (Kitana) - Serveur de jeu multijoueur
# Voir : https://github.com/Asso-MO5/kitana
VITE_KITANA_URL=ws://localhost:2567